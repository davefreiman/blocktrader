<div class="user-new-container">

<p><%= link_to "New Location", new_user_location_path(@user) %></p>


<% if @locations %>
<% @locations.each do |location | %>
	<p><%= link_to "Delete Location", [@user,location], :method => :delete %></p>
  	  <p> <%= location.address  %> </p>
  	<p> <%= location.latitude %> </p>
  	<p> <%= location.longitude %> </p>

  <%end%>
<% end %>  
</div>